{
	"name": "commertize-monorepo",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@10.13.1",
	"scripts": {
		"dev": "turbo run dev",
		"dev:landing": "turbo run dev --filter=@commertize/landing",
		"dev:dashboard": "turbo run dev --filter=@commertize/dashboard",
		"build": "turbo run build",
		"lint": "turbo run lint",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"migration:create": "turbo run migration:create --filter=@commertize/dashboard",
		"migration:up": "turbo run migration:up --filter=@commertize/dashboard"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@typescript-eslint/eslint-plugin": "^8.48.1",
		"@typescript-eslint/parser": "^8.48.1",
		"eslint": "^9.39.1",
		"eslint-config-next": "16.0.3",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"prettier": "^3.7.4",
		"turbo": "^2.6.3",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"dotenv-expand": "^12.0.3",
		"tsx": "^4.21.0"
	}
}
