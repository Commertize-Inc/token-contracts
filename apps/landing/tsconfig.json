{
	"extends": "../../tsconfig.nextjs.json",
	"compilerOptions": {
		"paths": {
			"@/*": ["./src/*"]
		},
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"**/*.mts",
		".next/types/**/*.ts",
		".next/dev/types/**/*.ts"
	]
}
