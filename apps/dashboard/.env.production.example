NODE_ENV=production

# Privy Configuration
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here
PRIVY_APP_SECRET=your_privy_app_secret_here

# NeonDB Configuration
DATABASE_URL=postgresql://user:password@your-neon-host/commertize?sslmode=require

# Plaid Configuration
# Get your credentials from: https://dashboard.plaid.com/team/keys
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_here
# Environment: sandbox, development, or production
PLAID_ENV=sandbox
# Required for Identity Verification - create template at: https://dashboard.plaid.com/identity-verification/templates
PLAID_IDENTITY_VERIFICATION_TEMPLATE_ID=your_template_id_here
