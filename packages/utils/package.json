{
	"name": "@commertize/utils",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"browser": {
		"fs": false,
		"path": false
	},
	"exports": {
		".": {
			"development": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./client": {
			"development": "./src/client.ts",
			"types": "./dist/client.d.ts",
			"import": "./dist/client.js",
			"require": "./dist/client.cjs"
		},
		"./server": {
			"development": "./src/server.ts",
			"types": "./dist/server.d.ts",
			"import": "./dist/server.js",
			"require": "./dist/server.cjs"
		},
		"./env-server": {
			"development": "./src/env-server.ts",
			"types": "./dist/env-server.d.ts",
			"import": "./dist/env-server.js",
			"require": "./dist/env-server.cjs"
		},
		"./onchain": {
			"development": "./src/onchain/index.ts",
			"types": "./dist/onchain/index.d.ts",
			"import": "./dist/onchain/index.js",
			"require": "./dist/onchain/index.cjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"lint": "eslint .",
		"clean": "rm -rf node_modules dist .turbo .next .vite coverage"
	},
	"dependencies": {
		"@commertize/nexus": "workspace:*",
		"add": "^2.0.6",
		"dotenv": "^17.2.3",
		"dotenv-expand": "^12.0.3",
		"fs": "0.0.1-security",
		"jose": "^6.1.3",
		"pnpm": "^10.25.0",
		"posthog-js": "^1.305.0",
		"posthog-node": "^5.17.2"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.19.26",
		"@types/react": "^18.0.0 || ^19.0.0",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}
